    "start": "npm run build && esbuild --servedir=dist",
    "test": "npm run build && test-browser-example test"
  },
  "browserslist": "last 1 Chrome version",
  "dependencies": {
    "@helia/unixfs": "^1.4.1",
    "helia": "next"
  },
  "devDependencies": {
    "esbuild": "^0.17.16",
    "rimraf": "^5.0.0"
  }
}
