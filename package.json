{
  "name": "www-helia-identify",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Run identify with a peer with Helia in a browser",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf ./dist",
